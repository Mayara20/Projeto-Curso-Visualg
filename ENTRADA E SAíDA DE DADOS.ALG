algoritmo "Entrada e Saída de Dados"
var
  nome: caractere
  idade: inteiro
inicio
      escreva ("Digite o seu nome: ")
      leia (nome)
      escreva ("Digite a sua idade: ")
      leia(idade)
      escreval("O nome digitado foi: ",nome)
      escreval("A idade digitada foi: ",idade)
fimalgoritmo